{
  "name": "wipit",
  "version": "1.0.0",
  "description": "package.json for heroku deployment",
  "scripts": {
    "build": "tsc",
    "postinstall": "npm install --prefix server && npm install --prefix client/wip-app && npm run build",
    "watch-ts": "tsc -w",
    "deploy": "git add -A && git commit -m Heroku && git push heroku master"
  },
  "engines": {
    "node": "16.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rudy451/wipit.git"
  },
  "author": "Rudy451",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Rudy451/wipit/issues"
  },
  "homepage": "https://github.com/Rudy451/wipit#readme",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/node": "^17.0.35",
    "@types/uuid": "^8.3.4",
    "typescript": "^4.6.4"
  }
}
